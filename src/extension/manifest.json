{
  "name": "VidSketch",
  "description": "Captures your desktop and allows for easily adding video overlays",
  "version": "3",
  "manifest_version": 2,
  "icons": {
    "16": "img/icon.png",
    "128": "img/icon.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "desktopCapture",
    "http://tomlagier.com/",
    "http://localhost:3100/",
    "audioCapture"
  ]
}
